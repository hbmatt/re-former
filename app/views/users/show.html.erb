<h1>Details for <%= @user.username %></h1>

<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> || <%= link_to 'Delete', user_path(@user), method: :delete, data: {confirm: "Really delete this user?"} %> || <%= link_to 'Back to User List', users_path %>